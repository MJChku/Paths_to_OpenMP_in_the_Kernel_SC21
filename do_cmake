
NK_CFLAGS="-fPIC -DNAUTILUS -DDEBUG -g -fno-omit-frame-pointer -ffreestanding -fno-stack-protector -fno-strict-aliasing -fno-strict-overflow -mno-red-zone -mcmodel=kernel"
#
NK_CXXFLAGS="$NK_CFLAGS"
NK_CPPFLAGS=""
NK_ASMFLAGS=""
NK_LDFLAGS=""
NK_LIBFLAGS=""

cmake  -DCMAKE_C_COMPILER=clang -DCMAKE_CXX_COMPILER=clang++-10 -DCMAKE_INSTALL_PREFIX=/home/446/openmp/ -DLIBOMP_ENABLE_SHARED=0 -DLIBOMP_OMPT_SUPPORT=FALSE -DLIBOMP_CXXFLAGS="$NK_CXXFLAGS" -DLIBOMP_CPPFLAGS="$NK_CPPFLAGS" -DLIBOMP_ASMFLAGS="$NK_ASMFLAGS" -DLIBOMP_LDFLAGS="$NK_LDFLAGS" -DLIBOMP_LIBFLAGS="$NK_LIBFLAGS" .. 

